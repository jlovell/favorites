#ios-native-bar.navbar-inverse
#ios-native-bar-placeholder.navbar-inverse

:css
  #ios-native-bar {
    width: 100%;
    height: 20px;
    display: none;
    position: fixed;
    z-index: 1;
  }
  #ios-native-bar-placeholder {
    width: 100%;
    height: 20px;
    display: none;
  }

:javascript
  $(document).on('ready page:load', function () {
    if (window.navigator.standalone) {
      $("#ios-native-bar").show();
      $("#ios-native-bar-placeholder").show();
    }
  })
